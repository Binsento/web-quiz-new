{
    "compilerOptions": {
    "target": "ES5",
    "module": "es2015",
    "allowJs": true,          
        "jsx": "react",
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "moduleResolution": "node"
    },
"include": ["src/**/*"]
}